<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="wrtn_urdf">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import wrtn_urdf urdf file -->
    <xacro:include filename="$(find wrtn_urdf)/urdf/wrtn_urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="wrtn_urdf.ros2_control.xacro" />


    <xacro:wrtn_urdf_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
